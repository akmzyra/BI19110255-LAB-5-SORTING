#include<iostream>
using namespace std;

int main(){

	int array[] = {4,2,0,3,4,0,4,1,2,1,3};
	int n = sizeof(array)/sizeof(array[0]);

	cout << "Array before sorting: [ 4 2 0 3 4 0 4 1 2 1 3 ]" << endl;

	sort(array, array+n);
	cout << "Array after sorting: [ ";

	for (int i = 0; i < n; ++i)
	cout << array[i] << " ";
	cout << "]";
	return 0;

	}
